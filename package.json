{
  "name": "rehatsebentar",
  "displayName": "RehatSebentar",
  "description": "Gentle reminders to help developers take breaks and stop on time",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rehatsebentar.test",
        "title": "RehatSebentar: Test Reminder"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rehatsebentar",
          "title": "RehatSebentar",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "rehatsebentar": [
        {
          "type": "webview",
          "id": "rehatsebentarView",
          "name": "Alarm"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
